<body>
    <div class="page" >        
        <div id="header">
            <div class="header-links">
                <a href="@System.Configuration.ConfigurationManager.AppSettings.Get("VirtualTerminalUrl")" class="one" title="Virtual Terminal" target="_blank">VIRTUAL TERMINAL</a> |
                <a href="@System.Configuration.ConfigurationManager.AppSettings.Get("OnlineResourceCenterUrl")" class="one" title="Online Resource Center" target="_blank">ONLINE RESOURCE CENTER</a>                
            </div>
           <div class="headerImage">@Html.ActionLinkImage("index", "products-services", "images/img_header.jpg", "National Merchant Center", null)</div> 
        </div>

        <div class="main">
