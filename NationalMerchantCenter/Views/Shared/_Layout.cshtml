    <script src="@Url.Content("~/scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script src="~/Scripts/concentCookies/purecookie.js"></script>

    <script type="text/javascript">
        $(function () {
            $('#slides').slides({
                generateNextPrev: false,
                play: 5000
            });
        });
    </script>

