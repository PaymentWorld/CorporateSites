@using Codebase.Website.Helpers;

<!DOCTYPE html>
<html>
<head>
    <title>National Merchant Center</title>

    <link href="@Url.Content("~/Content/5E1A2906-F51C-4915-84D1-5FD49ED9C18B/main-site.css")" rel="stylesheet" type="text/css" />
